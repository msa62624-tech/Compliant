{
  "name": "compliant-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Professional monorepo for Compliant Platform - NestJS + Next.js + Prisma",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "build:shared": "cd packages/shared && pnpm build",
    "test": "turbo run test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "lint": "turbo run lint",
    "test:all": "bash scripts/local-test.sh",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo run clean && rm -rf node_modules && find packages -name 'tsconfig.tsbuildinfo' -delete",
    "db:studio": "cd packages/backend && pnpm db:studio",
    "db:migrate": "cd packages/backend && pnpm db:migrate",
    "db:push": "cd packages/backend && pnpm db:push",
    "backend": "cd packages/backend && pnpm dev",
    "frontend": "cd packages/frontend && pnpm dev"
  },
  "dependencies": {
    "serverless-http": "^3.2.0",
    "@anthropic-ai/sdk": "^0.71.2",
    "@nestjs/common": "^11.1.12",
    "@nestjs/core": "^11.1.12",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/config": "^4.0.2",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/microservices": "^11.1.12",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.5",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/websockets": "^11.1.12",
    "@nuxt/opencollective": "^0.4.1",
    "@prisma/client": "^5.22.0",
    "@sendgrid/mail": "^8.1.6",
    "aws-sdk": "^2.1693.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.66.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cookie-parser": "^1.4.7",
    "cron": "^4.3.5",
    "fast-safe-stringify": "^2.1.1",
    "handlebars": "^4.7.8",
    "ioredis": "^5.9.2",
    "isomorphic-dompurify": "^2.35.0",
    "multer": "^2.0.2",
    "nest-winston": "^1.10.2",
    "nodemailer": "^7.0.12",
    "opencollective": "^1.0.3",
    "openai": "^6.16.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.17.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.9",
    "playwright": "^1.57.0",
    "prettier": "^3.8.0",
    "ts-node": "^10.9.2",
    "turbo": "^2.7.5",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "pnpm": {
    "overrides": {
      "tar": ">=7.5.3"
    }
  }
}
